
<div class="row">
	<div class= "small-12 columns">
		<h1>Sign In</h1>

		<%=form_tag sessions_path do %>
		<%= label_tag :email %>
		<%= text_field_tag :email, params[:email] %>
		<%= label_tag :password %>
		<%= password_field_tag :password, params[:password] %>
		<%= submit_tag "Sign In" %>

		<% end %>
	</div>
</div>